        <activity
            android:name=".BlackListActivity"
            android:label="黑名单"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name=".AboutActivity"
            android:label="关于"
            android:theme="@style/Theme.AppCompat.Light" />
